{
  "name": "bnbwebexpertise/laravel-paybox-gateway",
  "description": "Laravel Paybox gateway",
  "require": {
    "php": ">=5.5",
    "laravel/framework": "5.1.*|5.2.*|5.3.*|5.4.*|>=5.5",
    "lib-openssl": "*",
    "guzzlehttp/guzzle": ">=5.3"
  },
  "autoload": {
    "psr-4": {
      "Bnb\\PayboxGateway\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "license": "MIT",  
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^5.4",
    "mockery/mockery": "^0.9.5"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    },
    "laravel": {
      "providers": [
        "Bnb\\PayboxGateway\\Providers\\PayboxServiceProvider"
      ]
    }
  }
}
